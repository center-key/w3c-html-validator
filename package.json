{
   "name": "w3c-html-validator",
   "version": "0.5.0",
   "description": "A package for testing HTML files or URLs against the W3C validator",
   "license": "MIT",
   "type": "module",
   "main": "dist/w3c-html-validator.js",
   "repository": "github:center-key/w3c-html-validator",
   "homepage": "https://github.com/center-key/w3c-html-validator",
   "bugs": "https://github.com/center-key/w3c-html-validator/issues",
   "docs": "https://github.com/center-key/w3c-html-validator#readme",
   "author": "Thomas Davis <thomasalwyndavis@gmail.com>",
   "keywords": [
      "html",
      "html5",
      "validator",
      "w3c"
   ],
   "jshintConfig": {
      "esversion": 9,
      "strict": "implied",
      "eqeqeq": true,
      "undef": true,
      "unused": true,
      "varstmt": true,
      "node": true,
      "mocha": true
   },
   "scripts": {
      "lint": "   jshint spec",
      "pretest": "npm run lint && rimraf **/.DS_Store",
      "test": "   mocha spec"
   },
   "dependencies": {
      "commander": "~7.2",
      "superagent": "~6.1",
      "superagent-proxy": "~2.1"
   },
   "devDependencies": {
      "jshint": "~2.13",
      "mocha": "~9.0",
      "rimraf": "~3.0"
   }
}
